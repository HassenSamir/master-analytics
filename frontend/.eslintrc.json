{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
   "extends": [
        "eslint:recommended", 
        "plugin:react/recommended", 
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "@typescript-eslint"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}
